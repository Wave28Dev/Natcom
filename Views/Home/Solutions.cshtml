        }

        .carousel-inner .item.next {
            transform: translateX(33%)
        }

        .carousel-inner .item.prev {
            transform: translateX(-33%)
        }

        .carousel-inner .item.right,
        .carousel-inner .item.left {
            transform: translateX(0);
        }


        .carousel-control.left, .carousel-control.right {
